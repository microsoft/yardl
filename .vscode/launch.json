  "version": "0.2.0",
  "configurations": [
    {
      "name": "cli sandbox model",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/tooling/cmd/yardl",
      "cwd": "${workspaceFolder}/models/sandbox",
      "args": ["generate"]
    },
    {
      "name": "cli test model",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/tooling/cmd/yardl",
      "cwd": "${workspaceFolder}/models/test",
      "args": ["generate"]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Debug Python Unit Test",
      "type": "python",
      "request": "test",
      "justMyCode": true
    }
  ]
}
